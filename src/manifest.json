{
  "name": "Chrome拡張test",
  "description": "文字列変換",
  "version": "0.6",
  "manifest_version": 2,
  "default_locale": "en",
  "browser_action": {
    "default_title": "文字列変換"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
},
  "permissions": [
    "storage"
  ]
}